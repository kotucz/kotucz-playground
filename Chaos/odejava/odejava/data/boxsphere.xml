<?xml version="1.0" encoding="UTF-8"?>
<xode version="1.0r20" name="motorcycle"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://tankammo.com/xode/1.0r20/xode.xsd">
	
	<world>
	
	<space>
		<geom name="plane">
			<transform>
				<position x="-1" y="-5" z="4" />
				<!--<rotation>
					<euler x="134.3" y="12" z="0" aformat="degrees" />
				</rotation>-->
			</transform>
			
			<box
				sizex="100"
				sizey="2"
				sizez="100"
			/>
			
		</geom>
		
		<body name="chassis2">
			<transform>
				<position x="-5" y="10" z="0" />
			</transform>


			
			<geom name="chassisGeom1">
				<box
					sizex="3"
					sizey="0.5"
					sizez="0.5"
				/>
				
			</geom>
			<group>
				<transform>
					<position x="0" y="0" z="-5" />
				</transform>
			
				<geom name="chassisGeom1">
					<transform>
						<position x="0" y="0" z="-5" />
					</transform>
					<box
						sizex="0.5"
						sizey="7"
						sizez="0.5"
					/>
					
				</geom>			
			</group>
			
			<mass_shape density="1" />

		</body>
		
		<body name="chassis">
			<transform>
				<position x="0" y="10" z="6.73" />
			</transform>


			
			<geom name="chassisGeom">
				<box
					sizex="3"
					sizey="1.5"
					sizez="1"
				/>
				
			</geom>
			
			<mass_shape density="1" />

		</body>
		
		<body name="FrontLeft">
			<transform>
				<position x="-0.88" y="8.21" z="5.07" />
			</transform>


			
			<geom name="FrontLeftGeom">
				
				<sphere
					radius="1"
				/>
				
			</geom>
					
			<joint name="FrontLeftJoint">
        
                <link2 body="chassis" />
                
                <hinge2>
                	
                    <anchor x="0" y="0" z="0" />
    
                    <axis x="0" y="0" z="1"
						FMax="1000"
                    />
                    <axis x="0" y="1" z="0"
						LowStop="0"
						HiStop="0"
						SuspensionERP="1"
						SuspensionCFM="0.1"
					    FMax="100"
						
                    />

                </hinge2>
            </joint>	
				
				
			<mass_shape density="0.2" />
			
		</body>			
		</space>
	</world>
</xode>
